CUR_DIR := $(shell pwd)
MIDDLEWARE_DIR := $(CUR_DIR)/../../..
include $(MIDDLEWARE_DIR)/base.mak

LIB_FILE := $(shell find $(CUR_DIR)/$(CFG_SDK_TOOLCHAIN)/r_static -name '*.a')
LIB_FILE += $(shell find $(CUR_DIR)/$(CFG_SDK_TOOLCHAIN)/depend -name '*.a')
HEADER_FILE := $(shell find $(CUR_DIR) -name '*.h')

.PHONY: all clean install

all:    $(LIB_FILE) $(HEADER_FILE)
install: all
	$(AT)mkdir -p $(OUT_LIB_DIR)
	$(AT)mkdir -p $(OUT_HEADER_DIR)
	$(AT)cp -rf $(LIB_FILE) $(OUT_LIB_DIR)/
	$(AT)cp -rf $(HEADER_FILE) $(OUT_HEADER_DIR)/
# Clean this project
clean:
