<?xml version="1.0" encoding="UTF-8"?>
<view
    id="SET_PAGE"
    isusecommonskin="yes">

    <window
        id="SET_PAGE"
        top="0"
        left="0"
        width="320"
        height="240"
        normalskin=""
        transparent="yes"
        opacity="255"
        winlevel="0"
        isrelease="yes"
        onshow="PDT_UI_SET_OnShow"
        onhide="PDT_UI_SET_OnHide"
        ontimer="PDT_UI_SET_OnTimer"
        onevent="PDT_UI_SET_OnEvent">

        <groupbox
            id="SET_GROUPBOX_TITLE"
            top="0"
            left="0"
            width="320"
            height="48"
            isrelease="yes"
            normalskin="SKIN_COMMON_TITLE_BG">

            <label
                id="SET_LABEL_INFO"
                top="0"
                left="48"
                width="224"
                height="48"
                isrelease="yes"
                font="FONT22"
                text="STRING_SET_INFO"
                alignment="hcenter|vcenter"
                transparent="yes"
                normalskin="SKIN_COMMON_TITLE_TEXT"/>

        </groupbox>

        <scrollbar
            id="SET_SCROLLBAR"
            top="50"
            left="315"
            width="3"
            height="156"
            isrelease="yes"
            normalskin="SKIN_SET_SCORLLBAR_BG"
            slidres="SKIN_SET_SCORLLBAR_SLIDER"
            sliddircet="vertical"
            downarrownormal=""
            uparrowpress=""
            downarrowpress=""
            alignment="hcenter|vcenter"
            uparrownormal=""/>
        <label
            id="SET_LABEL_TOP"
            top="48"
            left="315"
            width="3"
            height="2"
            isrelease="yes"
            normalskin="SKIN_COMMON_BG"/>
        <label
            id="SET_LABEL_RIGHT"
            top="48"
            left="318"
            width="2"
            height="160"
            isrelease="yes"
            normalskin="SKIN_COMMON_BG"/>
        <label
            id="SET_LABEL_BOTTOM"
            top="206"
            left="315"
            width="3"
            height="2"
            isrelease="yes"
            normalskin="SKIN_COMMON_BG"/>

        <label
            id="SET_LABEL_LEFT"
            top="48"
            left="313"
            width="2"
            height="160"
            isrelease="yes"
            normalskin="SKIN_COMMON_BG"/>



        <listbox
            id="SET_LISTBOX"
            top="48"
            left="0"
            width="313"
            height="160"
            font="FONT18"
            isrelease="yes"
            normalskin="SKIN_SET_LISTBOX_ROW_NORMAL"
            activeskin="SKIN_SET_LISTBOX_ROW_NORMAL"
            rownormalskin="SKIN_SET_LISTBOX_ROW_NORMAL"
            rowselectskin="SKIN_SET_LISTBOX_ROW_ACTIVE"
            noframe="no"
            hlineheight="1"
            hlinecolor="0xFFC1C1BB"
            alignment="hcenter|vcenter"
            rownum="3"
            colnum="5"
            datamodel="DATAMODEL_SETTINGS"
            scrollbar="SET_SCROLLBAR"
            oncellselect="PDT_UI_SET_OnCellSelect"
            onkeydown="PDT_UI_SET_OnKeyDown">

            <listcol
                id="SET_LISTBOX_COL_LEFT"
                colwidth="6"
                colbinddb="no"/>

            <listcol
                id="SET_LISTBOX_COL_TITLE"
                coltype="text"
                colwidth="144"
                colbinddb="yes"
                coldbindex="0"
                colalignment="left|vcenter"
                fgidx="0"
                font="FONT18"/>
            <listcol
                id="SET_LISTBOX_COL_CONTENT"
                coltype="text"
                colwidth="127"
                colbinddb="yes"
                coldbindex="1"
                colalignment="right|vcenter"
                fgidx="0"
                font="FONT18"/>

            <listcol
                id="SET_LISTBOX_COL_ICON"
                coltype="image"
                colwidth="26"
                colbinddb="yes"
                coldbindex="2"
                colalignment="hcenter|vcenter"/>

            <listcol
                id="SET_LISTBOX_COL_RIGHT"
                colwidth="10"
                colbinddb="no"/>

        </listbox>

        <groupbox
            id="SET_GROUPBOX_TITLE_BELOW"
            top="208"
            left="0"
            width="320"
            height="32"
            isrelease="yes"
            normalskin="SKIN_COMMON_TITLE_BG">

            <label
                id="SET_LABEL_BACK"
                top="0"
                left="5"
                width="100"
                height="32"
                font="FONT18"
                isrelease="yes"
                text="STRING_BACK"
                alignment="hcenter|vcenter"
                transparent="yes"
                normalskin="SKIN_COMMON_TITLE_TEXT"/>

            <label
                id="SET_LABEL_MENU"
                top="0"
                left="110"
                width="100"
                height="32"
                isrelease="yes"
                font="FONT18"
                text="STRING_MENU"
                alignment="hcenter|vcenter"
                transparent="yes"
                normalskin="SKIN_COMMON_TITLE_TEXT"/>

            <label
                id="SET_LABEL_OK"
                top="0"
                left="220"
                width="100"
                height="32"
                isrelease="yes"
                font="FONT18"
                text="STRING_OK"
                alignment="hcenter|vcenter"
                transparent="yes"
                normalskin="SKIN_COMMON_TITLE_TEXT"/>

        </groupbox>

    </window>

</view>

