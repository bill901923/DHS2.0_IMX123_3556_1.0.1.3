menu "File Manager Configuration"

choice
	prompt "File Management Rule"
	help
		Select File Management Rule

	config DCF
		bool "DCF"
		help
			Design rule for Camera File system
	config DTCF
		bool "DTCF"
		help
			Date-Time rule for Camera File system
endchoice
config SUPPORT_CUSTOM_BACKUP_FILE
		bool "Support custom backup file name"
		help

config SUPPORT_FILE_MARKER
		bool "Support file marker"
		help

endmenu
